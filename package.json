    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^5.0.0",
    "lodash.merge": "^4.6.1",
    "markdownlint-cli": "^0.28.1",
    "mocha": "^9.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
